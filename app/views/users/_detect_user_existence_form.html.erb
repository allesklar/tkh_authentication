<%= simple_form_for user, url: detect_existence_users_path, :html => { class: 'form-horizontal' } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, value: 'proceed', :class => 'btn btn-primary' %>
  </div>
<% end %>
